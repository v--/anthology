% https://tex.stackexchange.com/questions/156049/how-can-i-get-cleveref-enumitem-to-print-not-item-e-but-proposition-1
\newlist{defenum}{enumerate}{1}
\setlist[defenum]{label=\alph*), ref=\theproposition~(\alph*)}
\crefalias{defenumi}{definition}

\newlist{thmenum}{enumerate}{1}
\setlist[thmenum]{label=\alph*), ref=\theproposition~(\alph*)}
\crefalias{thmenumi}{theorem}

\newlist{algenum}{enumerate}{1}
\setlist[algenum]{label=\alph*), ref=\theproposition~(\alph*)}
\crefalias{algenumi}{algorithm}

\newlist{exenum}{enumerate}{1}
\setlist[exenum]{label=\alph*), ref=\theproposition~(\alph*)}
\crefalias{exenumi}{example}
